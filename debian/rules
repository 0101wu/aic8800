#!/usr/bin/make -f

include /usr/share/dpkg/pkg-info.mk
include /usr/share/dpkg/architecture.mk

%:
	dh $@

override_dh_install:
	dh_install -paic8800-dkms --exclude=.gitignore src/* usr/src/aic8800-$(DEB_VERSION)/

override_dh_dkms:
	dh_dkms -V $(DEB_VERSION)

override_dh_builddeb:
	dh_builddeb -- -Zxz
